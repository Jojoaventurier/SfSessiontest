{% extends 'base.html.twig' %}

{% block title %}Modification du programme{% endblock %}

{% block body %}



<div class="wrapper">

    <div class="centered wrapper">
        <h1>Programme de la session :</h1>
        <h2>{{ session }}</h2>

        <div class="table">
            <table>
                <tr>
                    <th>Modules</th><th>Dur√©e</th><th>Supprimer</th>
                </tr>

                {% for program in programs %}
                <tr>
                    <td>{{ program.unit }}</td><td>{{ program.numberOfDays }} jours</td><td><a href="">Supprimer</a></td> <br>
                </tr>
                {% endfor %}
            </table>
        </div>
        
    </div>

    <h2>Ajouter un module au programme</h2>

    {{ form(formAddProgram) }}

</div>


{% endblock %}
